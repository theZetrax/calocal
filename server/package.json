{
  "name": "calocal-api",
  "version": "1.0.0",
  "description": "A fast api server built using nodejs and expressjs for calocal project.",
  "main": "build/index.js",
  "scripts": {
    "dev": "NODE_PATH=./src nodemon src/index.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./build",
    "start": "npm run build && node build/index.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "keywords": [],
  "author": "Zablon Dawit",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^7.4.4",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.40"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.4"
  }
