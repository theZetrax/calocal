{
   "type": "better-sqlite3",
   "database": "./data/db.sql",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/models/entity/**/*.ts"
   ],
   "migrations": [
      "src/models/migration/**/*.ts"
   ],
   "subscribers": [
      "src/models/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}